{
    "name": "Mouse Footprint Tailing",
    "version": "1.0",
    "permissions": ["activeTab", "declarativeContent"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contents.js"]
      }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "page_action": {
        "default_popup": "popup.html"
      },
    "description": "This is test!",
    "manifest_version": 2
}